{
    "name": "rawphp/raw-file-system",
    "type": "library",
    "description": "RawFileSystem is a collection of utility methods that help with working with the file system.",
    "keywords": [
        "files",
        "directories",
        "links"
    ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawFileSystem\\": "src/RawPHP/RawFileSystem/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        },
        "installer-paths": {
            "vendor/squizlabs/php_codesniffer/CodeSniffer/Standards/RawPHP": ["rawphp/raw-code-standards"]
        }
    },
    "minimum-stability": "dev"
}
